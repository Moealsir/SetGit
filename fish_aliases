if status is-interactive
    # Commands to run in interactive sessions can go here
end

alias c 'clear'
alias t 'touch'
alias l 'ls'
alias i 'sudo apt-get install'
alias v 'vim'
alias d 'trash'
alias cx 'chmod +x'
alias f 'fish'
alias x='betty *.c'

#go back to previous directories
alias b 'cd ..'
alias bb 'cd ../..'
alias b3 'cd ../../../'
alias b4 'cd ../../../../'
alias b5 'cd ../../../../../'

#Git aliases
alias ada 'git add *'
alias ad 'git add'
alias gb 'git branch'
alias st 'git status'
alias gco 'git checkout'
alias p 'git push'
alias pl 'git pull'
alias fc 'vi ~/.config/fish/config.fish'
